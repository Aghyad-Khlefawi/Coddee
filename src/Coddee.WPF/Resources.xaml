    <DataTemplate x:Key="ToastInformationTemplate">
        <Grid Margin="0 0 0 10">
            <Border Background="#2999E1">
                <Border.Effect>
                    <DropShadowEffect ShadowDepth="1"
                                      BlurRadius="5"
                                      Direction="-120"
                                      Color="Black" />
                </Border.Effect>
            </Border>
            <DockPanel Margin="10 5">
                <Path Style="{StaticResource Shape_Information}"
                      Height="25"
                      VerticalAlignment="Center"
                      Width="25" />
                <TextBlock Margin="10 0"
                           Text="{Binding Message}"
                           VerticalAlignment="Center"
                           TextAlignment="Center"
                           FontSize="16"
                           TextWrapping="Wrap"
                           Foreground="White" />
